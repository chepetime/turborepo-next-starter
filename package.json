{
  "name": "turborepo-next-starter",
  "version": "1.0.0",
  "description": "This is an official starter Turborepo.",
  "license": "MIT",
  "author": {
    "name": "Jos√© Manuel Gulias Lugo",
    "email": "me@joselugo.dev",
    "url": "https://github.com/chepetime"
  },
  "homepage": "https://github.com/chepetime/turborepo-next-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chepetime/turborepo-next-starter.git"
  },
  "bugs": {
    "url": "https://github.com/chepetime/turborepo-next-starter/issues"
  },
  "main": "turbo.json",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=web",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "clean": "rm -rf **/node_modules && rm -rf **/.turbo",
    "maintenance": "turbo run maintenance"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "eslint": "^8.46.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.0.0",
    "turbo": "latest"
  },
  "keywords": [
    "next",
    "starter",
    "tailwind",
    "turborepo"
  ],
  "packageManager": "pnpm@8.6.10"
}
